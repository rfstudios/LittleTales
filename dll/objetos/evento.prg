/*   Este archivo contiene el proceso de eventos*/process evento(int id_obj,int an)	private		int cant;		int accion,valor1,valor2,valor3,valor4,valor5,valor6;	end	begin		ganimacion=an;		cant=buscar("contar",6);				/* 		   Usamos la variable local file, porque ya está declarada y este evento no la va a usar para nada.		   Como vamos a necesitar una variable local, pues usamos esa. 		*/		for(file=1;file<=cant;file++)		log("Gameobject "+ id_obj+": "+an+" "+ganimacion);			accion=buscar("accion",6);			valor1=buscar("valor1",6);			valor2=buscar("valor2",6);			valor3=buscar("valor3",6);			valor4=buscar("valor4",6);			valor5=buscar("valor5",6);			valor6=buscar("valor6",6);						switch(accion)				/* Grupo 1 */					case 11:						mostrarmensaje(valor1);					end					case 12:						esperar(valor1);					end					case 13:						go_to(valor1,valor2);					end					case 14:						elecciones(valor1,valor2,valor3,valor4,valor5);					end									/* Grupo 2 */					case 21:						borrar_evento();					end					case 22:						pasar_pagina();					end					case 23:						atras_pagina();					end									/* Grupo 3 */					case 31:						cambiar_dinero(valor1,valor2);					end					case 32:						cambiar_objetos(valor1,valor2,valor3);					end					case 33:						/* cambiar_grupo(); */					end									/* Grupo 4 */					case 41:						guardar();					end					case 42:						cargar();					end									/* Grupo 5 */					case 51:						teletransportar(valor1,valor2,valor3,valor4);					end					case 52:						cambiar_pos_evento(valor1,valor2,valor3,valor4);					end					case 53:						cambiar_grafico_mapa(valor1);					end					case 54:						/* cambiar_tono_niebla(); */					end					case 55:						cambiar_transparencia(valor1,valor2,valor3);					end					case 56:						mover_personaje(valor1,valor2);					end					case 57:						asignar_camara(valor1);					end									/* Grupo 6 */					case 61:						cambiar_tono_pantalla(valor1,valor2,valor3,valor4);					end					case 62:						flash_pantalla();					end					case 63:						sacudir_pantalla(valor1);					end									/* Grupo 7 */					case 71:						mostrar_imagen(valor1,valor2,valor3,valor4,valor5,valor6);					end					case 72:						mover_imagen();					end					case 73:						rotar_imagen();					end					case 74:						cambiar_tono_imagen();					end					case 75:						borrar_imagen();					end					case 76:						mostrar_animacion(valor1,valor2,valor3,valor4);					end									/* Grupo 8 */					case 81:						cambiar_musica(valor1);					end					case 82:						volumen_musica(valor1);					end					case 83:						reproducir_sonido(valor1);					end					case 84:						guardar_musica();					end									/* Grupo 9 */					case 91:						iniciar_tienda();					end									/* Grupo 10 */					case 101:						cambiar_pv();					end					case 102:						cambiar_aa();					end					case 103:						cambiar_estado();					end					case 104:						curar_todo();					end					case 105:						cambiar_exp();					end					case 106:						cambiar_nivel();					end					case 107:						cambiar_car();					end					case 108:						cambiar_habilidades();					end					case 109:						cambiar_equipamiento();					end					case 1010:						cambiar_grafico_personaje();					end									/* Grupo 11 */					case 111:						llamar_menu();					end					case 102:						menu_guardar();					end					case 103:						game_over();					end					case 104:						llamar_mainmenu();					end									/* Grupo 12 */					case 121:						set_variable(valor1,valor2);					end					case 122:						si_variable(valor1,valor2,valor3,valor4);					end					case 123:						cambiar_variable(valor1,valor2,valor3);					end			end			if(file<0) break; end						frame;		end			OnExit		ganimacion=false;		bloq=false;	end